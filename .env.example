# Security
CRON_SECRET=replace-with-long-random-string
ADMIN_API_TOKEN=replace-with-long-random-string

# Upstash Redis (preferred on Vercel)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Redis Cloud / Redis URL (alternative)
REDIS_URL=

# Alternative Vercel KV env names (optional)
KV_REST_API_URL=
KV_REST_API_TOKEN=

# Optional, only for telegram channel
TELEGRAM_BOT_TOKEN=

# Optional, only for browser web push channel
# Generate with: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
VAPID_SUBJECT=mailto:your-email@example.com
